{
  "name": "signal-remains",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Interactive map visualization of The Cure's tour history with AI-generated contextual insights",
  "author": "Chris Mahlke <cmahlke@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:api": "nodemon api/index.js",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev\" --names \"API,VITE\" --prefix-colors \"blue,green\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "npm run start:api",
    "start:api": "node api/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "update:check": "npm outdated"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/inter": "^5.2.8",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@reduxjs/toolkit": "^2.11.2",
    "@vis.gl/react-google-maps": "^1.7.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "lucide-react": "^0.561.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.25",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^15.15.0",
    "nodemon": "^3.1.11",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^5.4.21"
  }
}
